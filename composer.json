{
    "name": "freemarketamilitia/passify-pro",
    "description": "A WordPress plugin for integrating Google Wallet passes",
    "type": "project",
    "autoload": {
        "psr-4": {
            "PassifyPro\\": "includes/"
        }
    },
    "require": {
        "php": ">=7.4",
        "monolog/monolog": "^2.0",
        "google/apiclient": "^2.14"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "post-install-cmd": [
            "Composer\\Config::disableProcessTimeout"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "unstable",
    "license": "MIT",
    "authors": [
        {
            "name": "Jason Sandoz",
            "email": "jaysandoz92@gmail.com"
        }
    ]
}
